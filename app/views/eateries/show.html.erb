<h2> Welcome to <%= @eatery.name %> </h2>

<% @eatery.dishes.each do |dish| %>

    <div class="test card bg-dark text-white w-85 mt-2 mb-2">
    <img class="card-img" src="https://images.unsplash.com/photo-1504674900247-0877df9cc836?ixlib=rb-1.2.1&auto=format&fit=crop&w=1050&q=80" alt="Card image">
    <div class="card-img-overlay">
    <h5 class="card-title"> <%= dish.name %> </h5>
    <p class="card-text"> <%= dish.description %> </p>
    <p class="card-text"> <%= dish.price.to_i.to_s %> </p>
    </div>
    </div>

<% end %>

